CoreASM ParserTest

use Standard
use AoASMPlugin

init TestCallPointcut

rule Start = skip

aspect ParserTest {
	
	function static aa : -> STRING initially "asd" 
	
	rule asd = skip

	pointcut a(a1) : b(b1)

	pointcut b(b1) : call("X" as var, "[0-9]*" as nmb) by agen without return
	
	advice test(var, nmb) before : a(a1) { print "test( "+ nmb +" )"}	
 
}

rule X(a) =
	print a

rule TestCallPointcut = {
	X(4)
	print "Hallo Welt!"
}