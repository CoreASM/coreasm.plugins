CoreASM ParserTest

use Standard
use AopASM

init TestCallPointcut

rule Start = skip

aspect ParserTest {
	
	function static aa : -> STRING initially "asd" 
	
	rule asd = skip

	pointcut a(a1) : b(b1)

	pointcut b(b1) : call("X" as var, "[0-9]*" as nmb) by agen without return
	
	advice test(a1) before : a(a1) { print "test( "+ a1 +" )"} 	
 
}

rule X(a) =
	print b

rule TestCallPointcut =
	X(4)