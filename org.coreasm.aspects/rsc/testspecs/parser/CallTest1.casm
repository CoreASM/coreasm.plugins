CoreASM ParserTest

use Standard
use AoASMPlugin

init TestCallPointcut

rule Start = skip

aspect ParserTest {
	
	function static aa : -> STRING initially "asd" 
	
	rule asd = skip

	pointcut a(a2) : b(a2)

	pointcut b(nmb) : call("X" as var, "[0-9]*" as nmb) by agent without return
	
	advice test(var, nmb) before : a(nmb) { print "test( "+ nmb +" )"} 

} 

rule X(a) =
	print a 
	
rule test(a,b) = skip//X(a(asd)) 

rule TestCallPointcut = {
	X(4)
	print "Hallo Welt!"
}