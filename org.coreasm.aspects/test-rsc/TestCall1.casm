CoreASM ParserTest

use Standard
use AoASMPlugin

init TestCallPointcut

rule Start = skip

aspect ParserTest {

	function static aa : -> STRING initially "asd"

	rule asd = skip

	pointcut a(a1, a2) : b(a1, a2)

	pointcut b(fn, nmb) : call("X" as fn, "[0-9]*" as nmb) by agent without return

	advice test(nmb) before : a(fn, nmb) { print "test( "+ nmb +" )"}

}

rule X(a) =
	print a

rule test(a,b) = skip//X(a(asd))

rule TestCallPointcut = {
	X(4)
	print "Hallo Welt!"
}