CoreASM TestProceed1

use Standard
use AoASMPlugin

init Test

aspect ParserTest {

	//@require "TestProceed1 a\n6\nTestProceed1 b\n6"
	advice TestCall1(a) around : args(".*" as a,".*") or args(".*", ".*",".*" as a) or call("Z" as a) {
		seqblock
			print "TestProceed1 a"
			proceed
			print "TestProceed1 b"
		endseqblock
	}
}

rule X(a, b) = {
	print a / b
}

rule Y(a, b, c) = {
	print b / c
}
rule Z =
	print "Z"

rule Test = {
	X(42,7)
	Y(1,4,2)
	Z
}
