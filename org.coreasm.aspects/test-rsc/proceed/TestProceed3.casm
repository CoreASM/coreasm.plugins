CoreASM TestProceed1

use Standard
use AoASMPlugin

init Test

aspect ParserTest {

	//@require "TestProceed3 a\n6\nTestProceed3 b\nTestProceed3 a\nZ\nTestProceed3 b\nTestProceed3 a\n2\nTestProceed3 b"
	advice TestProceed3(a) around : args(".*" as a,".*") or args(".*", ".*",".*" as a) or call("Z" as a) {
		seqblock
			print "TestProceed3 a"
			proceed
			print "TestProceed3 b"
		endseqblock
	}
}

rule X(a, b) = {
	print a / b
}

rule Y(a, b, c) = {
	print b / c
}
rule Z =
	print "Z"

rule Test = {
	X(42,7)
	Y(1,4,2)
	Z
}
