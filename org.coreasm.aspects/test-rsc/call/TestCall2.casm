CoreASM TestCall2

use Standard
use AoASMPlugin

init TestCallPointcut

aspect ParserTest {

	//@require "before_test1"
	advice test1 before : call("myRuleCall", ".*",".*") { print "before_test1"}
	
	// @require "after_test1"
	advice test1 after : call("myRuleCall", ".*",".*") { print "after_test1"}

}

rule TestCallPointcut = {
	//@require "Hallo Welt!"
	myRuleCall("Hallo Welt!", 42)
	print "TestCallPointcut"
}

rule myRuleCall(a, b) =
	print a + " " + b